# Copy to .env and add your keys
# Required variables are marked with [REQUIRED]

# Database (local default works for docker-compose)
DATABASE_URL=postgresql://torale:torale@localhost:5432/torale

# Temporal Configuration
# For local development (docker-compose): Use defaults below
# For production (Temporal Cloud): Set TEMPORAL_HOST to your cloud endpoint and provide API key
TEMPORAL_HOST=localhost:7233
TEMPORAL_NAMESPACE=default
# TEMPORAL_API_KEY is REQUIRED for Temporal Cloud connections
# Get your API key from https://cloud.temporal.io/
# Leave empty for self-hosted Temporal (local docker-compose)
TEMPORAL_API_KEY=

# Security [REQUIRED]
# Generate with: openssl rand -hex 32
SECRET_KEY=

# Clerk Authentication [REQUIRED] - Get keys from https://dashboard.clerk.com
CLERK_PUBLISHABLE_KEY=pk_test_...
CLERK_SECRET_KEY=sk_test_...

# AI API Keys
# GOOGLE_API_KEY is REQUIRED for grounded search - Get from https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=
# Optional fallback providers
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Notifications (optional - for future external notifications)
NOTIFICATION_API_KEY=

# GCP Deployment (only needed for production deployment)
GCP_PROJECT_ID=your-project-id
CLOUD_RUN_REGION=us-central1